{
  "name": "api-rest-node",
  "version": "1.0.0",
  "description": "",
  "homepage": ".",
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js --watch",
    "start2": "cross-env NODE_ENV=production forever server.js",
    "test": "cross-env NODE_ENV=test jest --watchAll",
    "sequelize": "cross-env NODE_ENV=production npx sequelize db:migrate",
    "sequelize_dev": "cross-env NODE_ENV=development npx sequelize db:migrate",
    "sequelize_test": "cross-env NODE_ENV=test npx sequelize db:migrate",
    "sequelize_undo": "cross-env NODE_ENV=test npx sequelize db:migrate:undo",
    "sequelize_undo_all": "cross-env NODE_ENV=test npx sequelize db:migrate:undo:all",
    "sequelize_specific": "cross-env NODE_ENV=test npx sequelize db:migrate:undo:all --to XXXXXXXXXXXXXX-create-users.js",
    "seq_seed": "cross-env NODE_ENV=test npx sequelize seed:generate --name users",
    "seq_seed_undo": "cross-env NODE_ENV=test npx sequelize db:seed:undo",
    "seq_seed_undo_all": "cross-env NODE_ENV=test npx sequelize db:seed:undo:all",
    "seq_seed_specific": "cross-env NODE_ENV=test npx sequelize db:seed:undo --seed name-of-seed-as-in-data",
    "apidoc": "apidoc -e /node_modules -e /public -o ./doc node ./src/app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evertonlf9/api-node-express"
  },
  "author": "Everton Ferreira",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "faker": "^4.1.0",
    "jest-cli": "^25.4.0",
    "node-api-doc-generator": "^1.1.1",
    "nodemon": "^2.0.3",
    "sequelize-cli": "^5.5.1",
    "superagent": "^5.2.2",
    "supertest": "^4.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "csurf": "^1.11.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.4.0",
    "forever": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.9.10",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.6",
    "session-file-store": "^1.4.0",
    "socket.io": "^2.3.0",
    "uuid": "^7.0.3"
  }
}
